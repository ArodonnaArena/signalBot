{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@latest"
    }
  },
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/webhook"
    },
    {
      "src": "/health",
      "dest": "/api/webhook"
    },
    {
      "src": "/",
      "dest": "/api/index"
    }
  ]
}
